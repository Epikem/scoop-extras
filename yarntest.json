    "persist": [
        "cache",
        "bin",
        "mirror"
    ],
    "post_install": [
        "yarn config set cache-folder \"$dir\\cache\"",
        "yarn config set yarn-offline-mirror \"$dir\\mirror\"",
        "yarn config set prefix \"$dir\""
    ],
    "env_add_path": [
        "bin",
        "Yarn\\bin"
    ],
    "checkver": {
        "url": "https://yarnpkg.com/latest-version",
        "re": "([\\d.]+)"
    },
    "autoupdate": {
