    "version": "1.9.4",
    "suggest": {
        "Node.js": [
            "nodejs",
            "nodejs-lts"
        ],
        "NVM": [
            "nvm"
        ]
    },
    "url": "https://s3.ap-northeast-2.amazonaws.com/epikem-files/yarn-1.9.4-unsigned.msi",
